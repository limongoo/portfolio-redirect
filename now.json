{
    "version": 2,
    "name": "ivanlimongan-redirect",
    "alias": [
        "www.ivanlimongan.com"
    ],
    "routes": [
        { 
            "src": "/(.*)", 
            "status": 301, 
            "headers": 
                {
                    "Location": "https://ivanlimongan.com/$1"
                }
        }
    ]
}